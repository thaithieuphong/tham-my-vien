<div class="container-fluid">
	<div class="row">
		{{#each customers}}
		<div class="col-xl-3 col-lg-3 col-md-4 col-sm-6 col-xs">
			<div class="card mt-3 mb-3 text-center">
				{{#if this.image}}
				<figure class="figure">
					{{!-- <img src="{{this.image.url}}" class="figure-img img-fluid rounded" alt="..."> --}}
					<img src="{{this.image.url}}" class="figure-img img-thumbnail rounded" alt="...">
				</figure>
				{{!-- <img src="{{this.image.url}}" class="card-img-top" alt="..."> --}}
				{{else}}
				<img src="/img/user-icon.png" class="card-img-top" alt="...">
				{{/if}}
				<div class="card-body">
					<h5 class="card-title"><a href="/operating-room/nursing/customers/{{this._id}}/detail"
							class="text-success">{{this.firstName}} {{this.lastName}}</a></h5>
					<div class="d-flex justify-content-between">
						<label for="status">
							<strong>Ngày sinh:</strong>&nbsp;
						</label>
						<label
						class="text-primary form-label text-right">{{formatBirth this.birth}}</label>
					</div>
					<div class="d-flex justify-content-between">
						<label for="status">
							<strong>Giới tính:</strong>&nbsp;
						</label>
						<label
						class="text-primary form-label text-right">{{this.gender}}</label>
					</div>
					<div class="d-flex justify-content-between">
						<label for="status">
							<strong>Số điện thoại:</strong>&nbsp;
						</label>
						<label
						class="text-primary form-label text-right">0{{this.phone}}</label>
					</div>
					<div class="d-flex justify-content-between">
						<label for="status">
							<strong>Email:</strong>&nbsp;
						</label>
						<label
						class="text-primary form-label text-right">{{this.email}}</label>
					</div>
					<div class="d-flex justify-content-between">
						<label for="status">
							<strong>Địa chỉ:</strong>&nbsp;
						</label>
						<label
						class="text-primary form-label text-right">{{this.address}}</label>
					</div>
					<div class="d-flex justify-content-between">
						<label for="status">
							<strong>Nguồn:</strong>&nbsp;
						</label>
						<label
						class="text-primary form-label text-right">{{this.resource}}</label>
					</div>
					<div class="d-flex justify-content-between">
						<label for="status">
							<strong>Mô tả:</strong>&nbsp;
						</label>
					</div>
					<label class="text-primary">{{this.description}}</label>
				</div>
				<div class="card-footer bg-transparent text-center">
					<button type="button" class="btn btn-primary btn-mobile mb-1" data-bs-toggle="modal"
						data-bs-target="#edit-customer-modal" data-id="{{this._id}}" data-edit-img="{{this.image.url}}"
						data-edit-firstname="{{this.firstName}}" data-edit-lastname="{{this.lastName}}"
						data-edit-birth="{{this.birth}}" data-edit-gender="{{this.gender}}"
						data-edit-phone="{{this.phone}}" data-edit-email="{{this.email}}"
						data-edit-address="{{this.address}}" data-edit-resource="{{this.resource}}" data-edit-description="{{this.description}}">
						Sửa thông tin
					</button>
					<button type="button" class="btn btn-success btn-mobile mb-1" data-bs-toggle="modal"
						data-service-note-id="{{this._id}}" data-service-note-img="{{this.image.url}}"
						data-service-note-firstname="{{this.firstName}}" data-service-note-lastname="{{this.lastName}}"
						data-service-note-birth="{{this.birth}}" data-service-note-gender="{{this.gender}}"
						data-service-note-phone="{{this.phone}}" data-service-note-email="{{this.email}}"
						data-service-note-address="{{this.address}}"
						data-service-note-description="{{this.description}}" {{#each this.serviceNoteID}}
						data-service-note-serviceID="{{this.service}}" {{/each}}
						data-bs-target="#create-service-note-modal">
						Tạo phiếu
					</button>
				</div>
			</div>
		</div>
		{{/each}}
	</div>
</div>



<!-- Modal Create Customer -->
<div class="modal fade" id="create-customer-modal" tabindex="-1" aria-labelledby="create-customer-modal"
	aria-hidden="true">
	<div class="modal-dialog modal-lg">
		<div class="modal-content">
			<div class="modal-header bg-primary">
				<h5 class="modal-title text-light" id="create-customer-modal">Tạo thông tin khách hàng</h5>
				<button type="button" class="btn-close btn-close-white" data-bs-dismiss="modal"
					aria-label="Close"></button>
			</div>
			<div class="modal-body">
				<form id="create-customer-form" method="POST" class="row" enctype="multipart/form-data">
					<div class="col-xl-3 text-center mb-3">
						<img src="/img/user-icon.png" id="add-avt-customer" class="rounded mx-auto d-block add-avata"
							alt="...">
						<label for="input-create-img" class="label-avt btn btn-sm btn-primary mt-3">
							<h6>Chọn ảnh đại diện</h6>
						</label>
						<input hidden id="input-create-img" type="file" name="image"
							accept="image/png, image/gif, image/jpeg">
					</div>
					<div class="col-xl-9 mb-3">
						<ul class="list-group">
							<li class="list-group-item">
								<div class="row">
									<div class="col-md-6 mb-3">
										<label for="create-firstName" class="form-label">
											<h6>Họ <span class="badge text-danger">*</span></h6>
										</label>
										<input type="text" class="form-control form-control-sm" id="create-firstName"
											placeholder="Vd: Trần Văn" name="firstName" required>
									</div>
									<div class="col-md-6">
										<label for="create-lastName" class="form-label">
											<h6>Tên <span class="badge text-danger">*</span></h6>
										</label>
										<input type="text" class="form-control form-control-sm" id="create-lastName"
											placeholder="Vd: Khánh" name="lastName" required>
									</div>
								</div>
								<div class="row mt-3">
									<div class="col-md-6 mb-3">
										<label for="create-birth" class="form-label">
											<h6>Ngày sinh <span class="badge text-danger">*</span></h6>
										</label>
										<input type="date" class="form-control form-control-sm" id="create-birth"
											aria-describedby="birthFeedback" name="birth" required>
									</div>
									<div class="col-md-6">
										<label for="create-gender" class="form-label">
											<h6>Giới tính <span class="badge text-danger">*</span></h6>
										</label>
										<select class="form-select form-select-sm" aria-label=".form-select-sm example"
											name="gender" required>
											<option value="" selected>-- Chọn giới tính --</option>
											<option value="Nữ">Nữ</option>
											<option value="Nam">Nam</option>
										</select>
									</div>
								</div>
								<div class="row mt-3">
									<div class="col-md-6 mb-3">
										<label for="create-phone" class="form-label">
											<h6>Số điện thoại <span class="badge text-danger">*</span></h6>
										</label>
										<input type="tel" class="form-control form-control-sm" id="create-phone"
											placeholder="Vd: 0909888888" name="phone" required>
										<div id="create_phone_error" class="text-danger off mt-1">Vui lòng nhập đúng số điện thoại</div>
									</div>
									<div class="col-md-6">
										<label for="create-email" class="form-label">
											<h6>Email</h6>
										</label>
										<input type="email" class="form-control form-control-sm" id="create-email"
											placeholder="Vd: tranvankhanh@gmail.com" name="email" required>
									</div>
								</div>
								<div class="row mt-3">
									<div class="col-md-6 mb-3">
										<label for="create-address" class="form-label">
											<h6>Địa chỉ</h6>
										</label>
										<input type="text" class="form-control form-control-sm" id="create-address"
											aria-describedby="addressFeedback" placeholder="Vd: 123 Đường số 10"
											name="address" required>
									</div>
									<div class="col-md-6">
										<label for="create-resource" class="form-label">
											<h6>Nguồn khách</h6>
										</label>
										<input type="text" class="form-control form-control-sm" id="create-resource"
											aria-describedby="resource"
											name="resource" required>
									</div>
								</div>
								<div class="row mt-3">
									<div class="col-md-12 mb-3">
										<label for="create-description" class="form-label">
											<h6>Mô tả</h6>
										</label>
										<textarea class="form-control" id="create-description" rows="6"
											placeholder="Mô tả khách hàng" name="description"></textarea>
									</div>
								</div>
							</li>
						</ul>
					</div>
				</form>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Đóng</button>
				<button id="create-customer-btn" type="submit" class="btn btn-primary">Tạo mới</button>
			</div>
		</div>
	</div>
</div>

